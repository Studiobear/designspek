{
  "name": "site2",
  "version": "0.0.1",
  "scripts": {
    "dev": "run-p routify rollup",
    "dev:dyn": "cross-env BUNDLING=dynamic npm run dev",
    "build": "npm run routify -- -b && rollup -c",
    "serve": "spassr --serve-spa --serve-ssr",
    "export": "routify export",
    "build:docker": "npm run build && ./scripts/docker/docker-build.sh",
    "deploy:now": "cd scripts/now && npm run deploy",
    "deploy:netlify": "cd scripts/netlify && npm run deploy",
    "rollup": "rollup -cw",
    "routify": "routify -D"
  },
  "devDependencies": {
    "@openfonts/fira-sans_latin": "^1.44.1",
    "@openfonts/playfair-display_latin": "^1.0.1",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@studiobear/designspek": "*",
    "@studiobear/designspek-components": "*",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "del": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "ppid-changed": "^1.0.1",
    "rollup": "^2.6.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-livereload": "^1.2.0",
    "rollup-plugin-svelte": "^5.2.1",
    "rollup-plugin-terser": "^5.3.0",
    "spassr": "^1.0.2"
  },
  "dependencies": {
    "@sveltech/routify": "^1.5.9",
    "svelte": "^3.20.1"
  }
}
