{
  "name": "site",
  "description": "Svelte System UI website",
  "version": "0.2.0",
  "author": "Barry Low <barry@studiobear.co>",
  "license": "MIT",
  "scripts": {
    "develop": "run-p --race dev cy:open",
    "validate": "svelte-check --ignore src/node_modules/@sapper",
    "validate:dev": "svelte-check --ignore src/node_modules/@sapper --watch",
    "dev": "sapper dev --ext '.svelte .svx'",
    "debug": "ndb yarn dev",
    "build": "sapper build --legacy --ext '.svelte .svx'",
    "export": "sapper export --legacy --ext '.svelte .svx'",
    "start": "node __sapper__/build",
    "start:export": "npx serve __sapper__/export",
    "eslint-disabled": "eslint src",
    "eslint:fix-disabled": "eslint --fix src",
    "lint": "prettier --check 'src/**/*.{html,js,svelte}'",
    "lint:fix": "prettier --write 'src/**/*.{html,js,svelte}'",
    "jest": "jest src",
    "jest:watch": "yarn jest --watch src",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test-disabled": "run-p --race jest",
    "kill": "kill $(lsof -ti:3001,3000,3002)"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "polka": "^0.5.2",
    "sirv": "^2.0.2",
    "svelte-preprocess": "^4.10.4"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime": "^7.17.2",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.1",
    "@studiobear/designspek": "*",
    "@studiobear/designspek-components": "*",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/svelte": "^3.0.3",
    "@types/compression": "^1.7.2",
    "@types/node": "^17.0.21",
    "@types/polka": "^0.5.4",
    "babel-jest": "^27.5.1",
    "cypress": "^9.5.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "htmlparser2": "^7.2.0",
    "jest": "^27.5.1",
    "jest-transform-svelte": "^2.1.1",
    "mdsvex": "^0.10.5",
    "ndb": "^1.1.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "prismjs": "^1.27.0",
    "rehype-stringify": "^9.0.3",
    "remark": "^14.0.2",
    "remark-excerpt": "^1.0.0-beta.1",
    "remark-frontmatter": "^4.0.1",
    "remark-html": "^15.0.1",
    "remark-parse": "^10.0.1",
    "remark-parse-yaml": "^0.0.3",
    "remark-rehype": "^10.1.0",
    "rollup": "^2.68.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.29.3",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.5",
    "svelte-jester": "^2.3.2",
    "to-vfile": "^7.2.3",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "typography-theme-kirkham": "^0.16.19",
    "unified": "^10.1.1",
    "unist-util-filter": "^4.0.0",
    "unist-util-visit": "^4.1.0",
    "xtend": "^4.0.2"
  }
}
