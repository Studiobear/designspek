{
  "name": "site",
  "description": "Svelte System UI website",
  "version": "0.2.0",
  "author": "Barry Low <barry@studiobear.co>",
  "license": "MIT",
  "scripts": {
    "develop": "run-p --race dev cy:open",
    "validate": "svelte-check --ignore src/node_modules/@sapper",
    "validate:dev": "svelte-check --ignore src/node_modules/@sapper --watch",
    "dev": "sapper dev --ext '.svelte .svx'",
    "debug": "ndb yarn dev",
    "build": "sapper build --legacy --ext '.svelte .svx'",
    "export": "sapper export --legacy --ext '.svelte .svx'",
    "start": "node __sapper__/build",
    "start:export": "npx serve __sapper__/export",
    "eslint-disabled": "eslint src",
    "eslint:fix-disabled": "eslint --fix src",
    "lint": "prettier --check 'src/**/*.{html,js,svelte}'",
    "lint:fix": "prettier --write 'src/**/*.{html,js,svelte}'",
    "jest": "jest src",
    "jest:watch": "yarn jest --watch src",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test-disabled": "run-p --race jest",
    "kill": "kill $(lsof -ti:3001,3000,3002)"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "next",
    "sirv": "^1.0.6",
    "svelte-preprocess": "^4.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@studiobear/designspek": "*",
    "@studiobear/designspek-components": "*",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/svelte": "^3.0.0",
    "@types/compression": "^1.7.0",
    "@types/node": "^14.11.1",
    "@types/polka": "^0.5.1",
    "babel-jest": "^26.3.0",
    "cypress": "^5.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-svelte3": "^2.7.3",
    "htmlparser2": "^4.1.0",
    "jest": "^26.4.2",
    "jest-transform-svelte": "^2.1.1",
    "mdsvex": "^0.8.8",
    "ndb": "^1.1.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "prismjs": "^1.19.0",
    "rehype-stringify": "^8.0.0",
    "remark": "^12.0.1",
    "remark-excerpt": "^1.0.0-beta.1",
    "remark-frontmatter": "^2.0.0",
    "remark-html": "^12.0.0",
    "remark-parse": "^8.0.3",
    "remark-parse-yaml": "^0.0.3",
    "remark-rehype": "^7.0.0",
    "rollup": "^2.28.1",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-svelte": "^6.0.1",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.28.8",
    "svelte": "^3.26.0",
    "svelte-check": "^1.0.47",
    "svelte-jester": "^1.1.5",
    "to-vfile": "^6.0.0",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3",
    "typography-theme-kirkham": "^0.16.19",
    "unified": "^9.2.0",
    "unist-util-filter": "^2.0.1",
    "unist-util-visit": "^2.0.1",
    "xtend": "^4.0.2"
  }
}
