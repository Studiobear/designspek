{
  "name": "@studiobear/designspek-components",
  "description": "A complete, full-featured, unofficial Svelte port of Headless UI, an unstyled, fully accessible UI component library.",
  "keywords": [
    "svelte",
    "headlessui",
    "svelte3",
    "svelte-components",
    "headless",
    "sveltejs"
  ],
  "license": "MIT",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json --use-new-transformation true",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "peerDependencies": {
    "svelte": "^3.46.4"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@popperjs/core": "^2.11.2",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/svelte": "^3.0.3",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vanilla-extract/babel-plugin": "^1.1.5",
    "@vanilla-extract/css": "^1.6.8",
    "@vanilla-extract/vite-plugin": "^3.1.4",
    "babel-jest": "^27.5.1",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "jest": "^27.5.1",
    "jest-axe": "^5.0.1",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.5",
    "svelte-inline-compile": "^0.1.0",
    "svelte-jester": "^2.3.2",
    "svelte-popperjs": "^1.2.8",
    "svelte-preprocess": "^4.10.4",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "typescript-svelte-plugin": "^0.3.0",
    "vite": "^2.9.1"
  }
}
