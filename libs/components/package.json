{
  "name": "@studiobear/designspek-components",
  "version": "0.5.0",
  "description": "System UI components for Svelte",
  "author": "Barry Low <barry@studiobear.co>",
  "license": "MIT",
  "svelte": "src/index.ts",
  "module": "dist/m/index.js",
  "main": "dist/index.js",
  "umd": "dist/umd/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src/components",
    "src/index.ts",
    "README.md"
  ],
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "clean": "rm -rf dist",
    "eslint-disabled": "eslint src",
    "eslint:fix-disabled": "eslint --fix src",
    "lint": "prettier --check 'src/**/*.{html,js,svelte}'",
    "lint:fix": "prettier --write 'src/**/*.{html,js,svelte}'",
    "typecheck": "svelte-check",
    "dts": "tsc -d --emitDeclarationOnly --declarationDir dist/ts",
    "test": "jest src",
    "test:watch": "yarn test -- --watch",
    "storybook": "start-storybook -p 6006 -s ../../static",
    "build-storybook": "build-storybook",
    "prepublishOnly": "yarn typecheck && yarn build"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/addons": "^6.0.21",
    "@storybook/core": "^6.0.21",
    "@storybook/svelte": "^6.0.21",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/svelte": "^3.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/jest-axe": "^3.5.0",
    "deepmerge": "^4.2.2",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest-dom": "^3.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-svelte3": "^2.7.3",
    "esm": "^3.2.25",
    "jest": "^26.4.2",
    "jest-axe": "^4.0.0",
    "prettier": "^2.1.2",
    "prettier-plugin-svelte": "^1.4.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "regenerator-runtime": "^0.13.3",
    "rollup": "^2.33.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-svelte": "^6.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.29.4",
    "svelte-check": "^1.0.54",
    "svelte-jester": "^1.1.5",
    "svelte-preprocess": "^4.3.2",
    "svelte-typed-component": "^1.1.1",
    "ts-jest": "^26.4.3",
    "tslib": "^2.0.1",
    "typescript": "^4.0.5",
    "typography-theme-st-annes": "^0.16.19",
    "webpack": "^4.41.6"
  },
  "peerDependencies": {
    "svelte": "^3.26.0"
  },
  "keywords": [
    "svelte",
    "sveltejs",
    "svelte3",
    "svelte-components"
  ],
  "dependencies": {
    "@studiobear/designspek": "^0.5.0"
  }
}
