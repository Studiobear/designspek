{
  "name": "@studiobear/designspek-components",
  "version": "0.5.0",
  "description": "System UI components for Svelte",
  "author": "Barry Low <barry@studiobear.co>",
  "license": "MIT",
  "svelte": "src/index.ts",
  "module": "dist/m/index.js",
  "main": "dist/index.js",
  "umd": "dist/umd/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src/components",
    "src/index.ts",
    "README.md"
  ],
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "clean": "rm -rf dist",
    "eslint-disabled": "eslint src",
    "eslint:fix-disabled": "eslint --fix src",
    "lint": "prettier --check 'src/**/*.{html,js,svelte}'",
    "lint:fix": "prettier --write 'src/**/*.{html,js,svelte}'",
    "typecheck": "svelte-check",
    "dts": "tsc -d --emitDeclarationOnly --declarationDir dist/ts",
    "test": "jest src --detectOpenHandles --force-exit",
    "test:watch": "yarn test -- --watch",
    "storybook": "start-storybook -p 6006 -s ../../static",
    "build-storybook": "build-storybook",
    "prepublishOnly": "yarn typecheck && yarn build"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addons": "^6.4.19",
    "@storybook/core": "^6.4.19",
    "@storybook/svelte": "^6.4.19",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^3.0.0",
    "@types/jest-axe": "^3.5.3",
    "deepmerge": "^4.2.2",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "esm": "^3.2.25",
    "jest": "^27.5.1",
    "jest-axe": "^5.0.1",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "regenerator-runtime": "^0.13.9",
    "rollup": "^2.68.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.5",
    "svelte-jester": "^2.3.2",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.10.4",
    "svelte-typed-component": "^1.2.0",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "typography-theme-st-annes": "^0.16.19",
    "webpack": "^5.69.1"
  },
  "peerDependencies": {
    "svelte": "^3.26.0"
  },
  "keywords": [
    "svelte",
    "sveltejs",
    "svelte3",
    "svelte-components"
  ],
  "dependencies": {
    "@studiobear/designspek": "^0.5.0"
  }
}
